#!/usr/bin/env bash

help() {
    echo ""
    echo "usage: $0 port"
    echo ""
    exit 1
}


if [ $# -ne 1 ]; then
  help
fi

PORT=$1

function kill_named {
    PID=$(ps aux | grep -i rstudio-server | grep "${PORT}" | grep -v grep | awk '{print $2}')
    if [ "$PID" != "" ] ; then
	kill -9 "$PID" > /dev/null 2>&1
        res=$?
    else
	res=$NOT_FOUND
    fi
    return "$res"
}

kill_named

exit $?


 

